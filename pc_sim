#! /usr/bin/vvp
:ivl_version "12.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x638055dc68b0 .scope module, "tb_pc" "tb_pc" 2 3;
 .timescale -9 -12;
v0x638055dd81b0_0 .var "clk", 0 0;
v0x638055dd8280_0 .net "pc_out", 31 0, v0x638055dd7fb0_0;  1 drivers
v0x638055dd8350_0 .var "reset", 0 0;
S_0x638055dc6a40 .scope module, "uut" "pc" 2 7, 3 3 0, S_0x638055dc68b0;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "reset";
    .port_info 2 /OUTPUT 32 "pc_out";
v0x638055dc6c90_0 .net "clk", 0 0, v0x638055dd81b0_0;  1 drivers
v0x638055dd7fb0_0 .var "pc_out", 31 0;
v0x638055dd8090_0 .net "reset", 0 0, v0x638055dd8350_0;  1 drivers
E_0x638055dc6f60 .event posedge, v0x638055dd8090_0, v0x638055dc6c90_0;
    .scope S_0x638055dc6a40;
T_0 ;
    %wait E_0x638055dc6f60;
    %load/vec4 v0x638055dd8090_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 32;
    %assign/vec4 v0x638055dd7fb0_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x638055dd7fb0_0;
    %addi 4, 0, 32;
    %assign/vec4 v0x638055dd7fb0_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x638055dc68b0;
T_1 ;
    %vpi_call 2 14 "$dumpfile", "waveforms/pc.vcd" {0 0 0};
    %vpi_call 2 15 "$dumpvars" {0 0 0};
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638055dd81b0_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x638055dd8350_0, 0, 1;
    %delay 5000, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x638055dd8350_0, 0, 1;
    %delay 100000, 0;
    %vpi_call 2 20 "$finish" {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x638055dc68b0;
T_2 ;
    %delay 5000, 0;
    %load/vec4 v0x638055dd81b0_0;
    %inv;
    %store/vec4 v0x638055dd81b0_0, 0, 1;
    %jmp T_2;
    .thread T_2;
    .scope S_0x638055dc68b0;
T_3 ;
    %vpi_call 2 26 "$monitor", "Time = %0t | PC = %h", $time, v0x638055dd8280_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./tb/tb_pc.v";
    "./src/pc.v";
